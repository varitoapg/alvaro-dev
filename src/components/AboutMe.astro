---
import { aboutMe } from "../data/aboutMe";
import CardContainer from "./common/CardContainer.astro";
import StringFormater from "./common/StringFormater.astro";
---

<CardContainer>
  {
    aboutMe.map((paragraph) => (
      <StringFormater
        text={paragraph}
        className="mb-5 block text-xl text-main-text dark:text-main-text-dark"
      />
    ))
  }
</CardContainer>
